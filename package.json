{
  "name": "vscode-lang-styp",
  "displayName": "vscode-StylePatch",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "styp",
        "aliases": [
          "STYlePatch",
          "styp"
        ],
        "extensions": [
          ".styp"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "javascript-jsx",
        "aliases": [
          "jsx"
        ],
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      },
      {
        "id": "js-react",
        "aliases": [
          "react"
        ],
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "grammars": [
      {
        "language": "styp",
        "scopeName": "source.styp",
        "path": "./syntaxes/styp.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.js-react": "js-react",
          "meta.embedded.block.css": "css"
        }
      },
      {
        "language": "javascript-jsx",
        "scopeName": "source.javascript-jsx",
        "path": "./syntaxes/javascript-jsx.tmLanguage.json"
      },
      {
        "language": "js-react",
        "scopeName": "source.js-react",
        "path": "./syntaxes/js-react.tmLanguage.json"
      },
      {
        "language": "css",
        "scopeName": "source.css",
        "path": "./syntaxes/css.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
